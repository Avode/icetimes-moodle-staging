<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/organization/db" VERSION="2025112602" COMMENT="XMLDB file for local_organization plugin"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd">

    <TABLES>

        <TABLE NAME="local_organization_ou" COMMENT="Stores Organization Units for local_organization">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" />
                <FIELD NAME="fullname" TYPE="char" LENGTH="255" NOTNULL="true" />
                <FIELD NAME="shortname" TYPE="char" LENGTH="100" NOTNULL="true" />
                <FIELD NAME="oucode" TYPE="char" LENGTH="50" NOTNULL="false" />
                <FIELD NAME="address1" TYPE="char" LENGTH="255" NOTNULL="false" />
                <FIELD NAME="address2" TYPE="char" LENGTH="255" NOTNULL="false" />
                <FIELD NAME="address3" TYPE="char" LENGTH="255" NOTNULL="false" />
                <FIELD NAME="postcode" TYPE="char" LENGTH="10" NOTNULL="false" />
                <FIELD NAME="state" TYPE="char" LENGTH="50" NOTNULL="false" />
                <FIELD NAME="district" TYPE="char" LENGTH="100" NOTNULL="false" />
                <FIELD NAME="cohortid" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" />
                <FIELD NAME="coursecatid" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" />
                <FIELD NAME="commandantuserid" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" />
                <FIELD NAME="adminuserid" TYPE="int" LENGTH="10" NOTNULL="false" DEFAULT="0" />
                <FIELD NAME="deleted" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0" />
                <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" />
                <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" />
            </FIELDS>

            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id" />
                <KEY NAME="shortname_uk" TYPE="unique" FIELDS="shortname" />
            </KEYS>
        </TABLE>

    </TABLES>
</XMLDB>
